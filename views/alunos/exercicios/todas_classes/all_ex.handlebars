<script>
const createCarouselModule = (carouselID, showButtonID, hideButtonID) => {
  const showButton = document.getElementById(showButtonID)
  const hideButton = document.getElementById(hideButtonID)
  const carousel = document.getElementById(carouselID)

  showButton.addEventListener('click', function(){
  showButton.classList.add('d-none')
  carousel.classList.remove('d-none')
  hideButton.classList.remove('d-none')
})

hideButton.addEventListener('click', function(){
  showButton.classList.remove('d-none')
  carousel.classList.add('d-none')
  hideButton.classList.add('d-none')
})

}

createCarouselModule('carouselExampleDark1', 'showEx', 'hideEx')
createCarouselModule('carouselExampleDark2', 'showEx2', 'hideEx2')
createCarouselModule('carouselExampleDark3', 'showEx3', 'hideEx3')
createCarouselModule('carouselExampleDark4', 'showEx4', 'hideEx4')

</script>
